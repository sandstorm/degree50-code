{
    "license": "MIT",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/ppatrik/OneloginSamlBundle"
        }
    ],
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/api-pack": "^1.1",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "easycorp/easyadmin-bundle": "^3.0",
        "firebase/php-jwt": "^5.2",
        "friendsofsymfony/ckeditor-bundle": "^2.2",
        "guzzlehttp/guzzle": "^6.3",
        "hslavich/oneloginsaml-bundle": "dev-master",
        "liip/imagine-bundle": "^2.3",
        "oneup/flysystem-bundle": "^3.4",
        "oneup/uploader-bundle": "^3.0",
        "opis/json-schema": "^1.0",
        "php-ffmpeg/php-ffmpeg": "^0.16.0",
        "ramsey/uuid": "^4.0",
        "rokka/imagine-vips": "^0.11.0",
        "sensio/framework-extra-bundle": "^5.5",
        "shivas/versioning-bundle": "^3.2",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/expression-language": "5.0.*",
        "symfony/flex": "^1.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/mercure-bundle": "^0.2",
        "symfony/messenger": "5.0.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/security-bundle": "5.0.*",
        "symfony/translation": "5.0.*",
        "symfony/webpack-encore-bundle": "^1.7",
        "symfony/yaml": "5.0.*",
        "vich/uploader-bundle": "^1.13",
        "webonyx/graphql-php": "^0.13.8"
    },
    "require-dev": {
        "api-platform/schema-generator": "^2.1",
        "carlosas/phpat": "^0.6.1",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "friends-of-behat/mink": "^1.8",
        "friends-of-behat/mink-browserkit-driver": "^1.4",
        "friends-of-behat/mink-extension": "^2.4",
        "friends-of-behat/symfony-extension": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.0.*",
        "symfony/css-selector": "5.0.*",
        "symfony/maker-bundle": "^1.11",
        "symfony/profiler-pack": "^1.0"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php56": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php71": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.0.*"
        }
    }
}
