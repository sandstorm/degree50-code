<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body>
        {{ include('Partials/Header.html.twig') }}
        <div class="main">
            <div class="main-inner">
                {{ include('Partials/FlashMessages.html.twig') }}
                {% block body %}{% endblock %}
            </div>
        </div>


        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
