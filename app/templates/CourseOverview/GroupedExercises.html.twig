{% for group in groupedExercises %}
    {% if group.exercises|length > 0 %}
        {% set groupIdTransVar = 'exerciseOverview.groups.' ~ (group.id) %}
        <h5>{{ groupIdTransVar | trans({}, 'DegreeBase') }}</h5>
        <ul class="overview">
            {% for exercise in group.exercises %}
                {{ include('CourseOverview/Exercise.html.twig', {exerciseWithStatusAndProgress: exercise}) }}
            {% endfor %}
        </ul>
    {% endif %}
{% endfor %}
